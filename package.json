{
  "name": "yesplaymusic",
  "version": "2.0.0",
  "description": "A nifty third-party NetEase Music player",
  "main": "dist/main/index.js",
  "homepage": "https://github.com/qier222/YesPlayMusic",
  "license": "MIT",
  "author": "qier222 <qier222@outlook.com>",
  "repository": "github:qier222/YesPlayMusic",
  "scripts": {
    "dev": "VITE_APP_ELECTRON=yes concurrently -n=vite,main -c=#646cff,#74b1be \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:renderer": "NODE_ENV=development vite --host 0.0.0.0",
    "dev:main": "NODE_ENV=development node -r ts-node/register script/build.main --watch",
    "build": "VITE_APP_ELECTRON=yes NODE_ENV=production npm run build:renderer && npm run build:main && electron-builder --config electron-builder.config.js --dir",
    "build:renderer": "NODE_ENV=production vite build",
    "build:main": "NODE_ENV=production node -r ts-node/register script/build.main",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "prettier": "npx prettier --write ./src ./script"
  },
  "dependencies": {
    "@sentry/tracing": "^6.17.4",
    "@sentry/vue": "^6.17.4",
    "@vueuse/core": "^7.5.5",
    "@vueuse/integrations": "^7.5.5",
    "NeteaseCloudMusicApi": "^4.5.3",
    "all-of-just": "^0.16.3",
    "axios": "0.25.0",
    "color.js": "^1.2.0",
    "colord": "^2.9.2",
    "dayjs": "^1.10.7",
    "electron-log": "^4.4.5",
    "electron-store": "^8.0.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-http-proxy": "^1.6.2",
    "howler": "^2.2.3",
    "js-cookie": "^3.0.1",
    "md5": "^2.3.0",
    "pinia": "^2.0.11",
    "pinia-plugin-persistedstate": "^1.2.1",
    "qrcode": "^1.5.0",
    "vue": "^3.2.29",
    "vue-gtag": "^2.0.1",
    "vue-i18n": "^9.2.0-beta.30",
    "vue-query": "^1.19.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.18",
    "@types/howler": "^2.2.4",
    "@types/minimist": "^1.2.2",
    "@types/wait-on": "^5.3.1",
    "@typescript-eslint/parser": "5.10.2",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.2.29",
    "autoprefixer": "^10.4.2",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "esbuild": "^0.14.18",
    "eslint": "8.8.0",
    "eslint-plugin-vue": "8.4.1",
    "minimist": "^1.2.5",
    "ora": "^5.4.1",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.5",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.49.7",
    "tailwindcss": "^3.0.18",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.15",
    "vite": "^2.7.13",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^0.31.1",
    "wait-on": "^6.0.0"
  }
}
